    "lint-staged": {
        "src/**/*.*(ts|tsx)": [
            "pretty-quick --staged",
            "npm run lint",
            "git add"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
