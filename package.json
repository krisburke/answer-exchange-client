        "history": "^4.9.0",
        "normalize.css": "^8.0.1",
        "react": "^16.8.2",
        "react-dom": "^16.8.2",
        "react-redux": "^6.0.0",
        "react-router-dom": "^4.3.1",
        "react-scripts": "2.1.5",
        "redux": "^4.0.1",
